;;;
;;; Test quote directly
;;;

;;; Test 1: Quote returns correct value
(print :quote-result)
(print (quote (a b c)))

;;; Test 2: Assign quote result to variable
(define v (quote (d e f)))
(print :variable-value)
(print v)

;;; Test 3: Pass quote result to primitive
(print :quote-to-primitive)
(print (car (quote (g h i))))

;;; Test 4: Build list containing quote result
(print :list-with-quote)
(print (cons (quote (j k l)) #42))

;;; Test 5: Pass that list to closure
(define id (lambda (x) x))
(print :list-through-closure)
(print (id (cons (quote (m n o)) #99)))
