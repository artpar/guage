;;;
;;; Test inline vs variable quoted values
;;;

(≔ id (λ (x) x))

;;; Test 1: Inline quoted value
(≋ :inline-quoted)
(≋ (id (⌜ (a b c))))

;;; Test 2: Variable quoted value
(≔ my-quoted (⌜ (d e f)))
(≋ :variable-quoted)
(≋ (id my-quoted))

;;; Test 3: Inline non-quoted list
(≋ :inline-list)
(≋ (id (⟨⟩ #1 #2)))

;;; Test 4: Variable non-quoted list
(≔ my-list (⟨⟩ #3 #4))
(≋ :variable-list)
(≋ (id my-list))
