;;;
;;; Test inline vs variable quoted values
;;;

(define id (lambda (x) x))

;;; Test 1: Inline quoted value
(print :inline-quoted)
(print (id (quote (a b c))))

;;; Test 2: Variable quoted value
(define my-quoted (quote (d e f)))
(print :variable-quoted)
(print (id my-quoted))

;;; Test 3: Inline non-quoted list
(print :inline-list)
(print (id (cons #1 #2)))

;;; Test 4: Variable non-quoted list
(define my-list (cons #3 #4))
(print :variable-list)
(print (id my-list))
