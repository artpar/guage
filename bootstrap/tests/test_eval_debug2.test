;;;
;;; Debug lambda evaluation
;;;

(load "bootstrap/stdlib/eval.scm")

(define empty-env (env-empty))

;;; Check structure of quoted lambda
(define lambda-expr (quote (lambda (:x) :x)))
(print lambda-expr)

;;; Check parts
(print (car lambda-expr))  ; Should be lambda
(print (cdr lambda-expr))  ; Should be ((:x) :x)
(print (car (cdr lambda-expr)))  ; Should be (:x) - params
(print (cdr (cdr lambda-expr)))  ; Should be (:x) - body
(print (car (cdr (cdr lambda-expr))))  ; Should be :x - body
