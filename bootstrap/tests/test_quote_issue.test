;;;
;;; Test what quoted expressions actually are
;;;

;;; Test 1: Create quoted list
(define list-a (quote (a b c)))
(print :what-is-list-a)
(print list-a)
(print (pair? list-a))

;;; Test 2: Try to get car of quoted list
(print :try-car)
(print (car list-a))

;;; Test 3: Create non-quoted list
(define list-b (cons :a (cons :b (cons :c nil))))
(print :what-is-list-b)
(print list-b)
(print (pair? list-b))
(print (car list-b))

;;; Test 4: Pass non-quoted list to function
(define get-first (lambda (x) (car x)))
(print :call-with-non-quoted)
(print (get-first list-b))

;;; Test 5: Pass quoted list to function
(print :call-with-quoted)
(print (get-first list-a))
