;;;
;;; Test if quoted values are stored correctly
;;;

(define my-quoted (quote (a b c)))

;;; Direct access works
(print :direct-access)
(print my-quoted)
(print (car my-quoted))

;;; Define identity AFTER the quoted value
(define id (lambda (x) x))

;;; Try through closure
(print :through-closure-after)
(print (id my-quoted))

;;; Now define quoted value AFTER identity
(define id2 (lambda (y) y))
(define my-quoted2 (quote (d e f)))

(print :direct-access-2)
(print my-quoted2)

(print :through-closure-before)
(print (id2 my-quoted2))
