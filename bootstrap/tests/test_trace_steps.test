;;;
;;; Trace evaluation steps
;;;

;;; Step 1: Create and test quote directly
(print :step1-create-quote)
(define qval (quote (a b c)))
(print qval)

;;; Step 2: Create identity function
(print :step2-create-id)
(define id (lambda (x) x))

;;; Step 3: Test with non-quote value first
(print :step3-test-with-number)
(print (id #42))

;;; Step 4: Test with the quote variable
(print :step4-test-with-qval)
(print (id qval))

;;; Step 5: Test with inline quote
(print :step5-test-with-inline)
(print (id (quote (d e f))))

;;; Step 6: Create a function that traces its argument
(print :step6-create-tracer)
(define tracer (lambda (y) (trace y)))

;;; Step 7: Call tracer with quote
(print :step7-call-tracer)
(tracer (quote (g h i)))
