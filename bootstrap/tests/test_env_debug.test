;;;
;;; Debug environment lookup
;;;

(load "bootstrap/stdlib/eval-env.scm")

;;; Create simple environment
(define env1 (((env-extend (env-empty)) :x) #42))

;;; Print environment structure
(print env1)

;;; Try to look up :x
(define lookup-fn (env-lookup env1))
(print lookup-fn)

;;; Now apply to :x
(define result (lookup-fn :x))
(print result)
