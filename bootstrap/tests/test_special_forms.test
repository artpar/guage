;;;
;;; Test if other special forms have the same issue
;;;

(define id (lambda (x) x))

;;; Test 1: Quote
(print :direct-quote)
(print (quote (a b c)))

(print :quote-through-closure)
(print (id (quote (a b c))))

;;; Test 2: Cons (primitive, not special form)
(print :direct-cons)
(print (cons :a :b))

(print :cons-through-closure)
(print (id (cons :a :b)))

;;; Test 3: If (special form)
(print :direct-if)
(print (if #t #42 #99))

(print :if-through-closure)
(print (id (if #t #42 #99)))
