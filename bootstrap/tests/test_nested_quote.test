;;;
;;; Test nested quotes
;;;

(define id (lambda (x) x))

;;; Test 1: Direct nested quote
(print :nested-quote-direct)
(print (quote (quote (a b c))))

;;; Test 2: Nested quote through closure
(print :nested-quote-closure)
(print (id (quote (quote (d e f)))))

;;; Test 3: Quote containing if
(print :quote-if-direct)
(print (quote (if #t #1 #2)))

;;; Test 4: Quote containing if through closure
(print :quote-if-closure)
(print (id (quote (if #t #3 #4))))
