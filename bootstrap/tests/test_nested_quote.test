;;; Test nested quotes
;;;

(≔ id (λ (x) x))

;;; Test 1: Direct nested quote
(≋ :nested-quote-direct)
(≋ (⌜ (⌜ (a b c))))

;;; Test 2: Nested quote through closure
(≋ :nested-quote-closure)
(≋ (id (⌜ (⌜ (d e f)))))

;;; Test 3: Quote containing if
(≋ :quote-if-direct)
(≋ (⌜ (? #t #1 #2)))

;;; Test 4: Quote containing if through closure
(≋ :quote-if-closure)
(≋ (id (⌜ (? #t #3 #4))))
