;;;
;;; Test passing quotes to different targets
;;;

;;; Test 1: Quote to primitive car (works)
(print :to-car-primitive)
(print (car (quote (a b c))))

;;; Test 2: Quote to primitive struct-create (type-of)
(print :to-type-of)
(print (struct-create (quote (d e f))))

;;; Test 3: Quote to primitive deep-equal? (deep-equal)
(print :to-deep-equal)
(print (deep-equal? (quote (g h)) (quote (g h))))

;;; Test 4: Define closure and pass quote
(print :to-closure)
(define f (lambda (x) x))
(print (f (quote (i j k))))

;;; Test 5: Nested closure
(print :to-nested-closure)
(define g (lambda (y) (lambda (z) z)))
(print ((g #1) (quote (l m n))))
