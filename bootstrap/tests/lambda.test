; Lambda Calculus Tests

; Test: Identity function
(define id (lambda (x) x))
(test-case (quote :identity) #42 (id #42))

; Test: Const function
(define const (lambda (x) (lambda (y) x)))
(test-case (quote :const) #10 ((const #10) #20))

; Test: Arithmetic in lambda
(define add1 (lambda (x) (+ x 1)))
(test-case (quote :add1) #42 (add1 #41))

; Test: Multiplication in lambda
(define double (lambda (x) (* x #2)))
(test-case (quote :double) #42 (double #21))
